<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="439.0" prefWidth="735.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.main">
   <children>
      <AnchorPane id="anchorParent" fx:id="anchorParent" prefHeight="439.0" prefWidth="735.0" stylesheets="@design/text.css">
         <children>
            <AnchorPane id="main_pane" fx:id="main_pane" layoutX="200.0" prefHeight="439.0" prefWidth="535.0" stylesheets="@design/style.css">
               <children>
                  <AnchorPane fx:id="dashboard_pane" prefHeight="439.0" prefWidth="535.0" visible="false">
                     <children>
                        <Label text="Dashboard" />
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="profile_pane" prefHeight="439.0" prefWidth="535.0">
                     <children>
                        <Label text="Profile" />
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="settings_pane" prefHeight="439.0" prefWidth="535.0" visible="false">
                     <children>
                        <Label text="Settings" />
                     </children>
                  </AnchorPane>
               </children></AnchorPane>
            <AnchorPane id="side_pane" prefHeight="439.0" prefWidth="200.0" stylesheets="@design/side-bar.css">
               <children>
                  <Button id="tab-selected" fx:id="side_profile" layoutY="25.0" mnemonicParsing="false" onAction="#profile_Click" prefHeight="25.0" prefWidth="200.0" stylesheets="@design/tabs.css" text="Profile" />
                  <Button id="tab-unselected" fx:id="side_settings" layoutY="50.0" mnemonicParsing="false" onAction="#settings_Click" prefHeight="25.0" prefWidth="200.0" stylesheets="@design/tabs.css" text="Settings" />
                  <Button id="tab-unselected" fx:id="side_dashboard" mnemonicParsing="false" onAction="#dashboard_Click" prefHeight="25.0" prefWidth="200.0" stylesheets="@design/tabs.css" text="Dashboard" />
               </children>
            </AnchorPane>
         </children></AnchorPane>
   </children>
</VBox>
