                  <Button id="tab-unselected" fx:id="side_dashboard" layoutY="27.0" mnemonicParsing="false" onAction="#dashboard_Click" prefHeight="25.0" prefWidth="200.0" stylesheets="@design/tabs.css" text="Dashboard" visible="false" />
               </children>
            </AnchorPane>
         </children>
         <stylesheets>
            <URL value="@design/text.css" />
            <URL value="@design/text-field.css" />
         </stylesheets></AnchorPane>
   </children>
</VBox>
